---
import { type CollectionEntry, getCollection } from 'astro:content';

import Layout from '../../layouts/group/Layout.astro';
import Container from '../../components/Container.astro';

export async function getStaticPaths() {
	const posts = await getCollection('teamlist_bod');
	return posts.map((post) => ({
		params: { slug: post.slug },
		props: post,
	}));
}
type Props = CollectionEntry<'project'>;

const post = Astro.props;
---

<Layout title={`${post.data.seo_title} | Board Of Director | Fullerton Health` } meta_description={`${post.data.seo_description}` } body_class="project inner index something you wish to add for any use">
	<article>
		<Container className="project-details">
			<div class="prose">
			</div>
		</Container>
	</article>
</Layout>
